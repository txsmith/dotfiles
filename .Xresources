! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.width: 480
rofi.lines: 10
! rofi.fuzzy: true
rofi.padding: 16
rofi.line-margin: 8
rofi.separator-style: solid
! rofi.hide-scrollbar: true
rofi.font: SFNS Display Regular 14
rofi.kb-remove-word-back: Control+BackSpace
rofi.color-window: argb:a0ffffff, argb:20282828, argb:20282828
rofi.color-normal: argb:00fdf6e3, #282828, argb:00eee8d5, argb:20282828, #282828
rofi.color-active: argb:00fdf6e3, #000000, argb:05eee8d5, argb:00d2b726, #fdf6e3
rofi.color-urgent: argb:00fdf6e3, #dc322f, argb:00eee8d5, argb:00dc322f, #fdf6e3